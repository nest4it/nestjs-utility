{
  "name": "@n4it/utility",
  "version": "1.0.0",
  "description": "Nest Utility modules for kickstarting projects",
  "license": "GPL-3.0",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "s": "npx nps",
    "s:list": "yarn s --scripts",
    "rebuild": "yarn clean && yarn build",
    "build": "yarn s build",
    "clean": "yarn s clean",
    "test": "npx jest --runInBand -c=jest.config.js packages/ --verbose",
    "lint": "eslint \"packages/**/*.ts\" --fix",
    "pub": "npx lerna publish --force-publish"
  },
  "keywords": [
    "nestjs",
    "nest",
    "utilities",
    "n4it"
  ],
  "dependencies": {},
  "devDependencies": {
    "lerna": "7.4.2",
    "nps-utils": "1.7.0",
    "nps": "5.10.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "prettier": "3.4.2",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "rimraf": "6.0.1",
    "jest": "29.7.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.0",
    "@types/supertest": "6.0.2",
    "@types/jsonwebtoken": "9.0.8",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "ts-toolbelt": "9.6.0",
    "passport": "0.7.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "reflect-metadata": "0.2.2",
    "@nestjs/common": "10.4.15",
    "@nestjs/event-emitter": "2.1.1",
    "@nestjs/core": "10.4.15",
    "@nestjs/testing": "10.4.15",
    "@nestjs/passport": "^10.0.3",
    "@types/passport": "1.0.17",
    "@nestjs/platform-express": "10.4.15",
    "supertest": "7.0.0"
  },
  "author": {
    "name": "n4it",
    "email": "hello@n4it.nl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nest4it/nestjs-utility"
  }
}
